<div id="genes-card" class="hide">
    <div class="card-label" >Genes</div>




    <div id="get-genes-dropdown" class="level-edu level-basic" style="margin-top:-5px;width: 125px;">
      <div class="btn-toolbar">
        <div class="btn-group" style="display:inline;">

          <button type="button" style="width:120px;font-size: 11px;padding-top: 2px;padding-bottom: 1px;" class=" icon-button-sm btn btn-default btn-raised dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
          <i class="material-icons" style="font-size:14px;padding-right:0px;padding-top:2px;">content_paste</i>Import Gene set<span style="margin-left:6px" class="caret"></span>
          </button>

          <ul class="dropdown-menu " role="menu">
            <li id="copy-paste-li">
              <div style="padding-left: 5px;padding-right: 5px;padding-top:3px;padding-bottom:3px;">
                <div>Copy/Paste</div>
                <textarea style="width:170px;height:150px;" id="genes-to-copy"></textarea>
                <div style="text-align:right">
                  <button id="get-genes-copy-paste-button" type="submit" onclick="genesCard.copyPasteGenes()" class="icon-button-sm btn btn-default btn-raised" style="margin-left:3px;padding-top:0px;padding-bottom:0px;margin-top:0px;margin-bottom: 1px;">
                    Done
                  </button>
                </div>
              </div>
            </li>

            <li class="todo hide "><a href="">Upload</a></li>
          </ul>
        </div>
      </div>
   </div>

  <!-- ACMG genes -->
  
          <button id="get-acmg-genes" type="submit" onclick="genesCard.ACMGGenes()" class="level-edu level-basic icon-button-sm btn btn-default btn-raised" style="width:120px;font-size:11px; margin-left:0px;padding-top:0px;padding-bottom:0px;margin-top:0px;margin-bottom: 10px;">
            <i class="material-icons" style="font-size:14px;padding-right:0px;padding-top:2px;">format_list_numbered</i>ACMG56 Genes<span style="margin-left:6px"></span>
          </button>


  <!-- Gene search box (mygene2 only) -->
  <div class="sidebar-label hide level-basic">Search for a gene (example: KDM1A)</div>
  <span id="bloodhound-sidebar" class="level-basic hide" >
      <input id="enter-gene-name-sidebar" class="level-edu typeahead" style="cursor:pointer;margin-top:0px;margin-bottom:0px;margin-right:10px" type="text" data-toggle="tooltip" data-placement="bottom" 
      placeholder="Gene">
  </span>     


  <!-- Phenolyzer -->          
   
    <div id="phenolyzer-search-box" >
      <div>
        <h4 class="level-edu level-basic">Phenolyzer</h4>
        <div class="sidebar-label hide level-basic">Search for a condition (examples: cystic fibrosis, breast cancer)</div>
        <h4 id="phenolyzer-title" class="hide">Phenolyzer</h4>
        

        <select id="select-phenotypes" class="level-edu" placeholder="Enter phenotype terms..."/>


        <select id="select-phenotype-edutour" class="level-basic" placeholder="Choose disease..." style="display:none;">
                <option value="breast_cancer">Breast cancer</option>
                <option value="colon_cancer">Colon cancer</option>                   
                <option value="b_cell_deficiency" >B Cell deficiency</option>                   
        </select>
        
        <div style="font-size:10px;margin-bottom:5px;">          
          <span id="powered-by-phenolyzer" class="hide level-basic">Powered by Phenolyzer</span>
          <div>
            For academic and non-profit use. 
            <a href="javascript:void(0)" data-toggle="collapse" data-target="#disclaimer" class="collapsed" aria-expanded="false">Disclaimer</a>
          </div>
        </div>

        <div style="text-align:center;">
        
          <button id="phenolyzer-go-button" type="submit" onclick="genesCard.getPhenolyzerGenes()" class="level-edu level-basic icon-button-sm btn btn-default btn-raised" style="line-height:normal;width:120px;margin-left:3px;padding-top:0px;padding-bottom:0px;margin-top:5px;margin-bottom: 10px;">
            <svg height="18" width="19">
              <g transform="translate(0,1)">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#search-dna-symbol" width="18" height="18" fill="#5d809d"></use>
              </g>
            </svg>


            <span id="phenolyzer-run-label" class="level-edu level-basic">Run</span>
            <span id="phenolyzer-run-label-long" class="level-edu level-basic hide">Get Genes</span>

          </button>
        </div>

